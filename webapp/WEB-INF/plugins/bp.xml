<?xml version="1.0" encoding="UTF-8"?>
<plug-in>
    <name>bp</name>
    <class>fr.paris.lutece.portal.service.plugin.PluginDefaultImplementation</class>
    <version>1.0.0</version>
    <documentation></documentation>
    <installation></installation>
    <changes></changes>
    <user-guide></user-guide>
    <description>bp.plugin.description</description>
    <provider>bp.plugin.provider</provider>
    <provider-url>http://your.web.site.com</provider-url>
    <icon-url>images/admin/skin/feature_default_icon.png</icon-url>
    <copyright>Copyright (c) 2013 Your Company</copyright>
    <db-pool-required>1</db-pool-required>
    
    <core-version-dependency>
        <min-core-version>4.1.0</min-core-version>
        <max-core-version/>	
    </core-version-dependency>

    <!-- Admin features parameters -->
    <admin-features>
        <admin-feature>
            <feature-id>BP_MANAGEMENT</feature-id>
            <feature-title>bp.adminFeature.ManageBp.name</feature-title>
            <feature-description>bp.adminFeature.ManageBp.description</feature-description>
            <feature-level>0</feature-level>
            <feature-url>jsp/admin/plugins/bp/ManageProjects.jsp</feature-url>
            <feature-icon-url></feature-icon-url>
      </admin-feature>
    </admin-features>    

    <!-- XPages parameters -->
    <applications>
        <application>
            <application-id>project</application-id>
            <application-class>fr.paris.lutece.plugins.bp.web.ProjectXPage</application-class>
        </application>
    </applications>

    <!-- Portlet parameters -->
    <portlets>
   </portlets>
</plug-in>